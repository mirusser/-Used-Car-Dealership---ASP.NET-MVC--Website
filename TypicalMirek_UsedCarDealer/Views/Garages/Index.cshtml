@model TypicalMirek_UsedCarDealer.Models.Garage

@{
    ViewBag.Title = "Index";
}

<h2>Your garage @User.Identity.Name</h2>

<table class="table">
    @if (Model.Orders != null)
    {
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Orders)
            </th>
            <th></th>
        </tr>

        foreach (var order in Model.Orders)
        {
            <tr>
                <td>
                    @Html.DisplayFor(model => order.CarId)
                </td>
                <td>
                    @Html.DisplayFor(model => order.DateOfOrder)
                </td>
            </tr>
        }
    }

</table>
